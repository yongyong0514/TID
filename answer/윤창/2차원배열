package problem;

public class TESTTEST {
	public static void main(String[]args) {
		int num[][] = new int[3][4];
		
		int value = 1;
		
		for(int i = 0; i < num.length; i++) {
			for(int j =0; j < num[i].length; j++) {
				num[i][j] = value++;
				System.out.printf("%2d ", num[i][j]);
			}
			System.out.println();
		}
	}
}
