class Solution {
    public String solution(int n) {
        String answer = "";
        
        for(int i = 1; i <= n; i++) {
            
            //홀수 짝수인지 나머지로 판단해서 홀과 짝을 구분지어 추가합니다.
            if(i % 2 != 0) {
                
                answer += "수";
                
            } else {
                
                answer += "박";
            }
        }      
        
        return answer;
    }
}
