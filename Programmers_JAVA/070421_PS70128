class Solution {
    public int solution(int[] a, int[] b) {
        int answer = 0;
        
        //배열의 길이만큼 반복하면서,
        for(int i = 0; i < a.length; i++) {
            
            //answer에 내적하는 값의 곱을 누적합니다.
            answer += a[i] * b[i];
        }
        
        return answer;
    }
}
